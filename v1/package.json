    "@peculiar/webcrypto": "^1.0.26",
    "@sentry/webpack-plugin": "^1.11.1",
    "@types/chai": "^4.2.11",
    "@types/classnames": "^2.2.10",
    "@types/firefox-webext-browser": "^78.0.1",
    "@types/mocha": "^7.0.2",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "abortcontroller-polyfill": "^1.4.0",
    "ajv-cli": "^3.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "doctoc": "^1.3.1",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mozilla": "^0.13.0",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-typescript": "^0.12.0",
    "fixpack": "^2.3.1",
    "fs-extra": "^3.0.1",
    "fx-runner": "^1.0.11",
    "geckodriver": "^1.19.1",
    "get-firefox": "^3.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "jshint": "^2.9.5",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "nock": "^13.0.1",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.11.0",
    "selenium-webdriver": "^3.6.0",
    "sinon": "4.0.0",
    "sinon-chrome": "2.2.1",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^5.0.0",
    "tailwindcss": "^1.1.3",
    "trash-cli": "^1.4.0",
    "ts-loader": "^6.2.2",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.8.3",
    "typescript-eslint-parser": "^20.0.0",
    "typescript-json-schema": "^0.42.0",
    "web-ext": "^4.3.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^3.1.1",
    "yamljs": "^0.3.0"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">=1.21.1",
    "node": ">=10.18.0"
  },
  "keywords": [
    "mozilla",
    "regrets-reporter"
  ],
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "build:development": "webpack && web-ext build",
    "build:production": "NODE_ENV=production webpack --mode=production && web-ext build",
    "clean": "trash build/${TARGET_BROWSER}/",
    "clean:test": "trash test/build",
    "compile-schema": "ajv compile -s telemetry-schema/regrets-reporter-update.1.deployed-schema.json -o ts/background.js/lib/validate.js",
    "dev-telemetry-server": "docker run -p 80:8000 -v \"$(pwd)/../../../mozilla-pipeline-schemas\"/schemas:/app/resources/schemas -it mozilla/edge-validator:latest",
    "docformat": "doctoc --title '**Contents**' docs/*.md && prettier '**/*.md' --trailing-comma=all --ignore-path=.eslintignore --write",
    "eslint": "eslint . --ext jsm,js,json",
    "eslint-fix": "npm run eslint -- --fix",
    "format": "prettier '**/*.{css,js,ts,tsx,json,jsm}' --trailing-comma=all --ignore-path=.eslintignore --write",
    "generate-manifest": "node generate-manifest.js",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "npm run eslint",
    "lint:fixpack": "fixpack",
    "lint:tslint": "tslint src/**/*.ts",
    "lint:web-ext-lint": "web-ext lint",
    "postformat": "npm run docformat && npm run eslint-fix && fixpack",
    "prebuild:development": "npm-run-all clean generate-manifest compile-schema",
    "prebuild:production": "NODE_ENV=production npm-run-all clean generate-manifest compile-schema",
    "prestart": "node ./ensure_minimum_node_version.js && npm run build:development",
    "pretest:unit:mocha": "NODE_ENV=test npx webpack --entry ./test/all-tests.ts --output-path test/build --output-filename all-tests.js --target node",
    "start": "TARGET_BROWSER=firefox web-ext run",
    "start:chrome": "TARGET_BROWSER=chrome web-ext run -t chromium",
    "test": "npm-run-all -n test:unit # test:func",
    "test:unit": "npm-run-all -n clean:test test:unit:mocha",
    "test:unit:mocha": "DOTENV_CONFIG_PATH=.env.development mocha -r source-map-support/register -r sinon/pkg/sinon -r ./test/browser-polyfills.ts -r dotenv/config --globals global test/build/all-tests.js",
    "watch": "TARGET_BROWSER=firefox npm-run-all build:development && TARGET_BROWSER=firefox concurrently 'webpack --watch' 'web-ext run'",
    "watch:chrome": "TARGET_BROWSER=chrome npm-run-all build:development && TARGET_BROWSER=chrome concurrently 'webpack --watch' 'web-ext run -t chromium'",
    "webpack:stats": "webpack --profile --json --mode=production > stats.json"
  },
  "packageManager": "yarn@3.1.1"
}
