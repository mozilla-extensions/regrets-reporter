    "pako": "^1.0.11",
    "photon-colors": "^3.3.1",
    "photon-icons": "^4.0.0",
    "react": "^16.13.1",
    "react-data-grid": "^7.0.0-canary.17",
    "react-dom": "^16.13.1",
    "react-popper": "^2.2.3",
    "react-virtualized": "^9.21.2",
    "semver": "^7.3.2",
    "typeface-changa": "^1.1.4",
    "typeface-nunito-sans": "^0.0.72",
    "typeface-zilla-slab": "^0.0.72",
    "webext-patterns": "^0.9.0",
    "webextension-polyfill-ts": "^0.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
